
<h1>Freelancer friendly places</h1>

<table>
  <tr>
    <th>Place name</th>
    <th>Location</th>
    <th>Rate</th>

  </tr>
    <% @places.each do |place| %>

      <tr>
        <td><%= place.name %></td>
        <td><%= place.adress %></td>
        <td><%= place.rate %></td>
        <td><%= link_to 'Show details', place_path(place) %></td>

      </tr>
    <% end %>
</table>
